<div class="panel panel-primary text-center container-fluid">
  <div class="panel-heading">
    <h1>All artists:</h1>
  </div>
  <div class="panel-body">

    <% @artists.each do |artist| %>
    <div class="list-group">
      <div class="list-group-item">
        <h4><%= link_to "#{artist.name}", artist%></h4>
      </div>

    </div>
    <% end %>

  </div>
</div>
